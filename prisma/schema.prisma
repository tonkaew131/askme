generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Question {
    id     String @id @default(auto()) @map("_id") @db.ObjectId
    title  String
    userId String @db.ObjectId
    user   User   @relation(fields: [userId], references: [id])
}

model User {
    id        String     @id @default(auto()) @map("_id") @db.ObjectId
    email     String
    questions Question[]
}
